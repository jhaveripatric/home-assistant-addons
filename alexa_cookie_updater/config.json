{
  "name": "Alexa Cookie Updater",
  "version": "1.0",
  "slug": "alexa_cookie_updater",
  "description": "Extracts Alexa cookie data and saves it for Node-RED.",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "once",
  "boot": "manual",
  "options": {
    "pickle_file_name": "alexa_media.YOUR_AMAZON_EMAIL.pickle"
  },
  "schema": {
    "pickle_file_name": "str"
  },
  "host_network": true,
  "map": ["config"]
}
